{
  "name": "demo-2",
  "version": "1.0.0",
  "description": "1. `git clone` to local folder\r 2. `npm start` from the root",
  "main": "index.js",
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "backend:install": "cd packages/backend && npm install",
    "frontend:install": "cd packages/frontend && npm install",
    "frontend:dev": "cd packages/frontend && npm run dev",
    "backend:dev": "cd packages/backend && npm run dev",
    "install": "concurrently \"npm run backend:install\" \"npm run frontend:install\"",
    "start": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reeyouz/demo-2.git"
  },
  "author": "Arijit Chowdhury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reeyouz/demo-2/issues"
  },
  "homepage": "https://github.com/reeyouz/demo-2#readme"
}
